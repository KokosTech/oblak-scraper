  "private": false,
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mariadb": "^3.1.1",
    "typeorm": "^0.3.14"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  }
